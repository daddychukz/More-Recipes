{
  "name": "more-recipes",
  "version": "1.0.0",
  "description": "A platform to share exciting recipe ideas",
  "main": "index.js",
  "scripts": {
    "server": "webpack -d && cp Client/src/index.html Client/dist/index.html && webpack-dev-server --content-base Client/src/ --inline --hot --history-api-fallback",
    "test": "npm run seed:db && nyc --reporter=html --reporter=text mocha ./Server/tests/api/user --compilers js:babel-register",
    "start": "sequelize db:migrate && nodemon ./Server/dist/app.js",
    "all": "npm run start & npm run server", 
    "setup:seed": "npm run migrations && sequelize db:seed:all",
    "seed:db": "npm run migrations && NODE_ENV=test sequelize db:seed:all",
    "migrations": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate",
    "build": "babel ./Server/src -d ./Server/dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daddychukz/More-Recipes.git"
  },
  "keywords": [
    "recipe"
  ],
  "author": "Chuks Durugo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/daddychukz/More-Recipes/issues"
  },
  "homepage": "https://github.com/daddychukz/More-Recipes#readme",
